<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/new_no_mian_two"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    >
	<ImageView 
	    android:id="@+id/new_action_image_bg_two"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:src="@drawable/weikaishibeijing_zhanwei"
	    android:scaleType="centerCrop"
	    />
    <com.lesports.stadium.view.MyScrollViews
        android:id="@+id/new_action_scrollview_two"
        android:layout_width="match_parent"
       android:background="#6e000000"
        android:layout_height="fill_parent" >

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:orientation="vertical" >
                <RelativeLayout
                    android:id="@+id/layout_new_nocation_data_s"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24.5dp"
                    android:layout_marginRight="24.5dp"
                    android:layout_marginTop="27dp"
                    android:background="#14ffffff" >

                    <ImageView
                        android:id="@+id/new_nocation_line_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/nocation_line" />

                    <TextView
                        android:id="@+id/new_nocation_tv_title_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/new_nocation_line_1"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="14dp"
                        android:text="测试标题"
                        android:textColor="@color/white"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/new_nocation_tv_title_xiao_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/new_nocation_tv_title_two"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="5dp"
                        android:text="从电视观看国内贷款看过上课"
                        android:textColor="@color/white"
                        android:textSize="11sp" />
                    <ImageView
                            android:id="@+id/new_nocation_line_2_two"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginTop="10dp"
                            android:layout_below="@+id/new_nocation_tv_title_xiao_two"
                            android:src="@drawable/nocation_line" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/layout_new_nocation_data_s_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24.5dp"
                    android:layout_marginRight="24.5dp"
                    android:background="#14ffffff" >


                    <RelativeLayout
                        android:id="@+id/new_nocation_layout_content_two"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                         >

                        

                        <TextView
                            android:id="@+id/new_nocation_tv_title_xiao_shuoming_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="11.5dp"
                            android:text="@string/noaction_huodongjieshao"
                            android:textColor="@color/white"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/new_nocation_tv_title_content_title_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/new_nocation_tv_title_xiao_shuoming_two"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="8dp"
                            android:text="活动内容中的标题"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/new_nocation_tv_content_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/new_nocation_tv_title_content_title_two"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="3dp"
                            android:lineSpacingExtra="10sp"
                            android:text="活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题活动内容中的标题"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <ImageView
                            android:id="@+id/new_nocation_line_3_two"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/new_nocation_tv_content_two"
                            android:layout_marginTop="16.5dp"
                            android:src="@drawable/nocation_line" />
                    </RelativeLayout>
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/new_action_layout_dangqianxuanting_two"
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:orientation="horizontal" >
                    <LinearLayout
                        android:id="@+id/new_noaction_layout_xuanchuan_table_two"
                        android:layout_width="match_parent"
                        android:layout_height="54dp"
                        android:layout_marginLeft="24.5dp"
                        android:layout_marginRight="24.5dp"
                        android:background="#14ffffff" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            <RelativeLayout
                                android:id="@+id/new_action_layout_xuanchuanshipin_two"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1" >

                                <TextView
                                    android:id="@+id/new_action_layout_xuanchuanshipin_tv_two"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginRight="38.5dp"
                                    android:layout_centerVertical="true"
                                    android:text="@string/noaction_shipin"
                                    android:textColor="#46cafd"
                                    android:textSize="16sp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/new_action_layout_xuanchuanhaibao_two"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1" >

                                <TextView
                                    android:id="@+id/new_action_layout_xuanchuanhaibao_tv_two"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="38.5dp"
                                    android:layout_centerVertical="true"
                                    android:text="@string/noaction_haibao"
                                    android:textColor="@color/white"
                                    android:textSize="16sp" />
                            </RelativeLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24.5dp"
                    android:layout_marginRight="24.5dp"
                    android:orientation="vertical" >

                    <com.lesports.stadium.view.MyGridView
                        android:id="@+id/new_action_grideview_picture_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:visibility="gone"
                        android:scrollbars="none"
	   				 	android:horizontalSpacing="10dp"
	    				android:verticalSpacing="10dp"
	   				 	android:listSelector="@null"
                        android:numColumns="3" />

                    <com.lesports.stadium.view.MyGridView
                        android:id="@+id/new_action_grideview_vedio_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:clickable="false"
                        android:horizontalSpacing="10dp"
	    				android:verticalSpacing="10dp"
	   				 	android:listSelector="@null"
                        android:numColumns="2" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/new_action_layout_xuantinghou_two"
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:orientation="horizontal" >
                <LinearLayout 
                    android:layout_width="match_parent"
                    android:layout_height="54dp"
                    android:orientation="horizontal"
                    android:background="#14ffffff"
                    android:layout_marginLeft="24.5dp"
               		 android:layout_marginRight="24.5dp"
                    >
                     <RelativeLayout
                    android:id="@+id/new_action_layout_xuanchuanshipins_two"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/new_action_layout_xuanchuanshipins_tv_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="38.5dp"
                        android:text="@string/noaction_shipin"
                        android:textColor="#46cafd"
                        android:textSize="16sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/new_action_layout_xuanchuanhaibaos_two"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" >

                    <TextView
                         android:id="@+id/new_action_layout_xuanchuanhaibaos_tv_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/noaction_haibao"
                        android:layout_marginLeft="38.5dp"
                        android:textColor="@color/white"
                        android:textSize="16sp" />
                </RelativeLayout>
                </LinearLayout>

               
            </LinearLayout>
        </FrameLayout>
    </com.lesports.stadium.view.MyScrollViews>

</RelativeLayout>