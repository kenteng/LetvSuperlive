<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#111a32"
    android:gravity="center" >
    
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        >

    <ListView
        android:id="@+id/all_keyishiyong_all_prize_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp"
        android:divider="#00000000"
        android:dividerHeight="15dp"
        android:gravity="center" />
        
    </RelativeLayout>
    <RelativeLayout 
        android:id="@+id/all_youhuiquanbukeyong_meishuju_y"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <ImageView
            android:id="@+id/all_youhuiquanbukeyong_meishujusss_y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="140dp"
            android:src="@drawable/wukeyongyouhuiquan" />

        <TextView
            android:id="@+id/all_youhuiquanbukeyong_meishujussssss_y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/all_youhuiquanbukeyong_meishujusss_y"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="24dp"
            android:textColor="@color/white"
            android:textSize="15sp"
            android:text="没有可用的优惠券" />
        
    </RelativeLayout>

    

</RelativeLayout>